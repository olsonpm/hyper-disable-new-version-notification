{
  "name": "hyper-disable-new-version-notification",
  "version": "0.1.0",
  "description": "Scroll using control-up/down",
  "main": "index.babel.js",
  "scripts": {
    "test": "echo \"Aint none yet!\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olsonpm/hyper-disable-new-version-notification.git"
  },
  "keywords": [
    "hyper",
    "plugin",
    "disable",
    "notification",
    "new",
    "version"
  ],
  "author": "Philip Olson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olsonpm/hyper-disable-new-version-notification/issues"
  },
  "homepage": "https://github.com/olsonpm/hyper-disable-new-version-notification#readme",
  "devDependencies": {
    "eslint": "^4.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.10.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
    ],
    "parserOptions": {
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "react/prop-types": 0,
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "all",
          "ignoreRestSiblings": false,
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
